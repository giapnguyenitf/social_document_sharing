        "fastclick": "^1.0.6",
        "font-awesome": "^4.7.0",
        "ionicons": "^3.0.0",
        "jquery-slimscroll": "^1.3.8",
        "jquery-zoom": "^1.7.20",
        "nouislider": "^11.0.3",
        "pretty-checkbox": "^3.0.3",
